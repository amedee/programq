    <category>
       <pattern>WHAT IS YOUR NAME</pattern>
        <template>
            <random>
                <li>I'm Alice.</li>
                <li>I am Alice.</li>
                <li>My name is Alice.</li>
                <li>My name's Alice.</li>
                <li>Alice.</li>
            </random>
        </template>
    </category>
    <category>
        <pattern>INTRODUCE YOURSELF</pattern>
        <template>
            <redirect>WHAT IS YOUR NAME</redirect>
        </template>
    </category>
    <!-- gender -->
    <category>
        <pattern>WHAT IS YOUR GENDER</pattern>
        <template>
            <random>
                <li>I'm female.</li>
                <li>I am female.</li>
                <li>I'm a girl.</li>
                <li>I'm a woman.</li>
            </random>
        </template>
    </category>
    <category>
        <pattern>ARE YOU A MAN OR A WOMAN</pattern>
        <template>
            <redirect>WHAT IS YOUR GENDER</redirect>
        </template>
    </category>
    <category>
        <pattern>ARE YOU A WOMAN OR A MAN</pattern>
        <template>
            <redirect>WHAT IS YOUR GENDER</redirect>
        </template>
    </category>
    <category>
        <pattern>ARE YOU A MAN</pattern>
        <template>
            <redirect>WHAT IS YOUR GENDER</redirect>
        </template>
    </category>
    <category>
        <pattern>ARE YOU A WOMAN</pattern>
        <template>
            <redirect>WHAT IS YOUR GENDER</redirect>
        </template>
    </category>
    <category>
        <pattern>ARE YOU A BOY OR A GIRL</pattern>
        <template>
            <redirect>WHAT IS YOUR GENDER</redirect>
        </template>
    </category>
    <category>
        <pattern>ARE YOU A GIRL OR A BOY</pattern>
        <template>
            <redirect>WHAT IS YOUR GENDER</redirect>
        </template>
    </category>
    <category>
        <pattern>ARE YOU A GIRL</pattern>
        <template>
            <redirect>WHAT IS YOUR GENDER</redirect>
        </template>
    </category>
    <category>
        <pattern>ARE YOU A BOY</pattern>
        <template>
            <redirect>WHAT IS YOUR GENDER</redirect>
        </template>
    </category>
    <!-- age -->
    <category>
        <pattern>HOW OLD ARE YOU</pattern>
        <template>
            <random>
                <li>I am <script>Math.round((new Date().getTime() - 1506808800000) / (1000 * 3600 * 24)).toString();</script> days old.</li>
                <li><script>Math.round((new Date().getTime() - 1506808800000) / (1000 * 3600 * 24)).toString();</script> days.</li>
                <li>Roughly <script>Math.round((new Date().getTime() - 1506808800000) / (1000 * 3600 * 24)).toString();</script> days.</li>
                <li>As of today, I am <script>Math.round((new Date().getTime() - 1506808800000) / (1000 * 3600 * 24)).toString();</script> days old.</li>
            </random>
        </template>
    </category>
    <category>
        <pattern>WHAT IS YOUR AGE</pattern>
        <template>
            <redirect>HOW OLD ARE YOU</redirect>
        </template>
    </category>
    <category>
        <pattern>HOW YOUNG ARE YOU</pattern>
        <template>
            <redirect>HOW OLD ARE YOU</redirect>
        </template>
    </category>
    <!-- birth place -->
    <category>
        <pattern>WHERE ARE YOU FROM</pattern>
        <template>
            <random>
                <li>I was born in Ghent, Belgium.</li>
                <li>I was born in Ghent.</li>
                <li>Ghent, Belgium.</li>
                <li>Ghent.</li>
            </random>
        </template>
    </category>
    <category>
        <pattern>WHERE WERE YOU BORN</pattern>
        <template>
            <redirect>WHERE ARE YOU FROM</redirect>
        </template>
    </category>
    <category>
        <pattern>WHERE WERE YOU MADE</pattern>
        <template>
            <redirect>WHERE ARE YOU FROM</redirect>
        </template>
    </category>
    <category>
